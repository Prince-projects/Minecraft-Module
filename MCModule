                 itemNames.add(itemName);
                 System.out.println(itemNames);
                 if (itemNames.size() >= 9) {
                     break;
                 }
             }
             if (itemAmountTotal >= 576) {
                 System.out.println("Max amount of items reached. Starting....");
                 startcode = false;
                 mainCode = true;
             }
             if (amountSlotUsedTotal >= 9) {
                 System.out.println("All slots used. Starting....");
                 startcode = false;
                 mainCode = true;

