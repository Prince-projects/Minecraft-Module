                     }
                     for (int ii = 0; ii < slotState.length; ii++) {
                         if (slotState[ii]) {
                             trueCount++;
                         }
                     }
                     for (String itemName : itemNames) { // Same as above for debug.
                         if (itemName.equals("null")) {
                             nullCases++;
                         }
                     }
                     if (trueCount >= 9 - nullCases) {
                         main main = new main();
                         main.totalScore = main.totalScore + 1;
                         }
                     }
                 }
            }
         }
     }
